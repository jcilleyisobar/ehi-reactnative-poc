<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:enterFadeDuration="@integer/selectors_animation_duration">
    <item
        app:state_range_middle="false"
        android:state_selected="true"
        android:drawable="@drawable/calendar_day_cell_highlighted_background"/>
    <item
        android:state_selected="true"
        app:state_range_middle="true"
        android:drawable="@drawable/calendar_day_cell_middle_background"/>
    <item
        android:state_selected="false"
        android:drawable="@drawable/calendar_day_cell_background"/>
</selector>
