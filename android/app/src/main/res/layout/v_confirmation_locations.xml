<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class="ConfirmationLocationsViewBinding" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.ehi.enterprise.android.ui.confirmation.widgets.ConfirmationLocationDetailsView
            android:id="@+id/pickup_location_details_view"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <com.ehi.enterprise.android.ui.confirmation.widgets.ConfirmationLocationDetailsView
            android:id="@+id/return_location_details"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </LinearLayout>

</layout>