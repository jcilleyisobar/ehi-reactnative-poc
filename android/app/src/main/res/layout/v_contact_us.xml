<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="ContactUsViewBinding" />

    <LinearLayout
        android:id="@+id/current_res_call_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="52dp"
        android:gravity="center"
        android:background="@drawable/green_button_touch_overlay"
        android:orientation="vertical"
        android:padding="@dimen/margin_medium">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/call_icon"
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:src="@drawable/icon_phone_01" />

            <TextView
                android:id="@+id/view_title"
                style="@style/TextViewSize18"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:textColor="@color/white"
                android:fontFamily="@font/source_sans_bold"
                tools:text="Title" />
        </LinearLayout>

        <TextView
            android:id="@+id/view_subtitle"
            style="@style/TextViewSize16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:fontFamily="@font/source_sans_light"
            tools:text="SubTitle" />

    </LinearLayout>

</layout>