<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data class="MapWrapperFragmentBinding" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.ehi.enterprise.android.ui.location.widgets.DragableMapView
            android:id="@+id/map_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.ehi.enterprise.android.ui.location.widgets.components.LocationMapBubbleDetailsView
            android:background="@color/white"
            android:visibility="invisible"
            android:id="@+id/location_details_view"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="56dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </RelativeLayout>
</layout>